<% if members.any? %>
  <ul class="list-unstyled">
    <% members.each do |member| %>
      <li class="media">
        <img class="mr-2 rounded" src="<%= gravatar_url(member, { size: 40 }) %>" alt="">
        <div class="media-body">
          <div>
            <%= member.name %>
          </div>
          <div>
            <p><%= link_to 'View profile', member_path(member) %></p>
          </div>
        </div>
      </li>
    <% end %>
  </ul>
  <%= paginate members %>
<% end %>